        <TextBlock x:Name="textBlock1_Copy" HorizontalAlignment="Center" Margin="69,157,59,0" TextWrapping="Wrap" Text="Report any issues to:" VerticalAlignment="Top" Foreground="#FF343432"/>
        <TextBlock x:Name="textBlock1_Copy1" TextAlignment="Center" Margin="20,178,20,0" TextWrapping="Wrap" Text="dave07747@gmail.com" VerticalAlignment="Top" Foreground="#FF343432"/>

