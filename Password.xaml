<Window x:Class="TowerSearch.Password"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Password" Height="136" Width="278" ResizeMode="NoResize" ShowInTaskbar="False" Topmost="True" Closed="Window_Closed_1">
    <Grid Background="#FF29A62E">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="227*"/>
        </Grid.ColumnDefinitions>
        <Label Content="Please enter password" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="1" Margin="0,22,106,56" Height="29" Width="166" FontSize="16"/>
        <PasswordBox x:Name="PassWord" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" Width="135" Height="25" PasswordChar="6" Background="#FF1A3E1B" Margin="14,51,123,31" Foreground="#DDFFFFFF" BorderBrush="#FF29A62E"/>
        <Button Grid.ColumnSpan="2" Content="Submit" HorizontalAlignment="Left" Margin="171,34,0,0" VerticalAlignment="Top" Background="#FF1A3E1B" Width="91" Click="Button_Click" RenderTransformOrigin="0.467,-0.55"/>
        <Button Grid.ColumnSpan="2" Content="Change Password" HorizontalAlignment="Left" Margin="133,75,-10,0" VerticalAlignment="Top" Width="149" Background="{x:Null}" Foreground="#FF1082E2" RenderTransformOrigin="0.44,0.9" Height="27" BorderBrush="{x:Null}" Click="Button_Click_1"/>

    </Grid>
</Window>
