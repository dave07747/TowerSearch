<Window x:Class="TowerSearch.PasswordDeleteLog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Please Enter Password" Height="136" Width="278" ResizeMode="NoResize" ShowInTaskbar="False" Topmost="True">
    <Grid Background="#FF29A62E">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="227*"/>
        </Grid.ColumnDefinitions>

        <PasswordBox x:Name="PassWord" Grid.ColumnSpan="2" HorizontalAlignment="Center" Margin="30,40,117,0" VerticalAlignment="Center" Width="125" Height="25" PasswordChar="6" Background="#FF1A3E1B" Foreground="#DDFFFFFF" BorderThickness="0"/>
        <Button Grid.ColumnSpan="2" Content="Submit" HorizontalAlignment="Left" Margin="179,46,0,0" VerticalAlignment="Top" Background="#FF1A3E1B" Width="83" Click="Button_Click"/>
        <TextBlock Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="-5,10,0,0" TextWrapping="Wrap" Text="Permanently Delete Log?" VerticalAlignment="Top" Width="184" TextAlignment="Center" Foreground="#DDFFFFFF" FontSize="18.667"/>

    </Grid>
</Window>
