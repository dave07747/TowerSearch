<Window x:Class="TowerSearch.PasswordEditParts"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Please Enter Password" Height="121" Width="278" ResizeMode="NoResize" ShowInTaskbar="False" Topmost="True" Closing="Window_Closing">
    <Grid Background="#FF29A62E">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="227*"/>
        </Grid.ColumnDefinitions>

        <PasswordBox x:Name="PassWord" Grid.ColumnSpan="2" HorizontalAlignment="Center" Margin="23,15,124,0" VerticalAlignment="Center" Width="125" Height="26" PasswordChar="6" Foreground="#DDFFFFFF" Background="#FF1A3E1B" BorderThickness="0"/>
        <Button Grid.ColumnSpan="2" Content="Submit" HorizontalAlignment="Left" Margin="177,26,0,0" VerticalAlignment="Top" Background="#FF1A3E1B" Width="78" Click="Button_Click"/>
        <TextBlock Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="23,13,0,0" TextWrapping="Wrap" Text="Password:" VerticalAlignment="Top" Width="125" TextAlignment="Center" Foreground="#DDFFFFFF" FontSize="18.667"/>

    </Grid>
</Window>
